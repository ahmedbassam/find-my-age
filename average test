fun main(args: Array<String>) {
    print("أدخل المعدل هنا: ")
    var avr:Float=readLine()!!.toFloat()

    if(avr>=95&&avr<100){
        println("متميز")}
    if(95>avr&&avr>=85){
        println("ممتاز")}
    if(85>avr&&avr>=75){
        println("جيد جدا")}
    if(75>avr&&avr>=65){
        println("جيد")}
    if(65>avr&&avr>=50){
        println("مقبول")}
    if(50>avr&&avr>=0){
        println("راسب")}
    if(avr>100||avr<0){
        println("إدخال خاطئ أعد التحقق")
    }


}
